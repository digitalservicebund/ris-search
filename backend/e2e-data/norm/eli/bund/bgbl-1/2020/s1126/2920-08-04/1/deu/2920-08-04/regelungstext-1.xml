        <akn:preamble GUID="b6c7d8e9-f0a1-4890-abcd-5678abcde1234" eId="preambel-1">Fiktiver einleitender Prolog zur Darstellung des Gesetzes.</akn:preamble>
        <akn:body GUID="c7d8e9f0-a1b2-4890-abcd-6789abcde123" eId="hauptteil-1">
            <akn:article GUID="d8e9f0a1-b2c3-4890-abcd-7890abcde123" eId="hauptteil-1_art-1" period="#meta-1_geltzeiten-1_geltungszeitgr-1" refersTo="stammform">
                <akn:num GUID="e9f0a1b2-c3d4-4890-abcd-890abcdef123" eId="hauptteil-1_art-1_bezeichnung-1">Art 1</akn:num>
                <akn:heading GUID="f0a1b2c3-d4e5-4890-abcd-901abcdef123" eId="hauptteil-1_art-1_überschrift-1">Fiktive Bestimmungen zur Einführung</akn:heading>
                <akn:paragraph GUID="a1b2c3d4-e5f6-4890-abcd-0123456789ac" eId="hauptteil-1_art-1_abs-1">
                    <akn:num GUID="b2c3d4e5-f6a7-4890-abcd-1234567890bd" eId="hauptteil-1_art-1_abs-1_bezeichnung-1">(1)</akn:num>
                    <akn:content GUID="c3d4e5f6-a7b8-4890-abcd-234567890cde" eId="hauptteil-1_art-1_abs-1_inhalt-1">
                        <akn:p GUID="d4e5f6a7-b8c9-4890-abcd-34567890cdef" eId="hauptteil-1_art-1_abs-1_inhalt-1_text-1">Die Einführung erfolgt gemäß den fiktiven Festlegungen dieses Gesetzes.</akn:p>
                    </akn:content>
                </akn:paragraph>
                <akn:paragraph GUID="e5f6a7b8-c9d0-4890-abcd-4567890cdef1" eId="hauptteil-1_art-1_abs-2">
                    <akn:num GUID="f6a7b8c9-d0e1-4890-abcd-567890cdef12" eId="hauptteil-1_art-1_abs-2_bezeichnung-1">(2)</akn:num>
                    <akn:content GUID="a7b8c9d0-e1f2-4890-abcd-67890cdef123" eId="hauptteil-1_art-1_abs-2_inhalt-1">
                        <akn:p GUID="b8c9d0e1-f2a3-4890-abcd-7890cdef1234" eId="hauptteil-1_art-1_abs-2_inhalt-1_text-1">Die Veröffentlichung erfolgt unmittelbar nach Beschlussfassung.</akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
            <akn:article GUID="c9d0e1f2-a3b4-4890-abcd-890cdef12345" eId="hauptteil-1_art-2" period="#meta-1_geltzeiten-1_geltungszeitgr-1" refersTo="stammform">
                <akn:num GUID="d0e1f2a3-b4c5-4890-abcd-901cdef12345" eId="hauptteil-1_art-2_bezeichnung-1">Art 2</akn:num>
                <akn:heading GUID="e1f2a3b4-c5d6-4890-abcd-0123def12345" eId="hauptteil-1_art-2_überschrift-1"/>
                <akn:paragraph GUID="f2a3b4c5-d6e7-4890-abcd-1234ef123456" eId="hauptteil-1_art-2_abs-1">
                    <akn:num GUID="a3b4c5d6-e7f8-4890-abcd-2345ef123456" eId="hauptteil-1_art-2_abs-1_bezeichnung-1">(1)</akn:num>
                    <akn:content GUID="b4c5d6e7-f8a9-4890-abcd-3456ef123456" eId="hauptteil-1_art-2_abs-1_inhalt-1">
                        <akn:p GUID="c5d6e7f8-a9b0-4890-abcd-4567ef123456" eId="hauptteil-1_art-2_abs-1_inhalt-1_text-1">Dieses Gesetz tritt am Tag nach der Verkündung in Kraft.</akn:p>
                    </akn:content>
                </akn:paragraph>
                <akn:paragraph GUID="d6e7f8a9-b0c1-4890-abcd-5678ef123456" eId="hauptteil-1_art-2_abs-2">
                    <akn:num GUID="e7f8a9b0-c1d2-4890-abcd-6789ef123456" eId="hauptteil-1_art-2_abs-2_bezeichnung-1">(2)</akn:num>
                    <akn:content GUID="f8a9b0c1-d2e3-4890-abcd-7890ef123456" eId="hauptteil-1_art-2_abs-2_inhalt-1">
                        <akn:p GUID="a9b0c1d2-e3f4-4890-abcd-8901ef123456" eId="hauptteil-1_art-2_abs-2_inhalt-1_text-1">Das Bundesministerium des Innern, für Bau und Heimat kündigt den Zeitpunkt der Umsetzung an.</akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
            <akn:article GUID="c9d0e2f2-a3b4-4890-abcd-890cdef12345" eId="hauptteil-1_art-3" period="#meta-1_geltzeiten-1_geltungszeitgr-2" refersTo="stammform">
                <akn:num GUID="d0e2f2a3-b4c5-4890-abcd-901cdef12345" eId="hauptteil-1_art-3_bezeichnung-1">Art 3</akn:num>
                <akn:heading GUID="e2f2a3b4-c5d6-4890-abcd-0123def12345" eId="hauptteil-1_art-3_überschrift-1"> Zukünftiger Artikel </akn:heading>
                <akn:paragraph GUID="f2a3b4c5-d6e7-4890-abcd-2234ef123456" eId="hauptteil-1_art-3_abs-1">
                    <akn:num GUID="a3b4c5d6-e7f8-4890-abcd-2345ef223456" eId="hauptteil-1_art-3_abs-1_bezeichnung-1">(1)</akn:num>
                    <akn:content GUID="b4c5d6e7-f8a9-4890-abcd-3456ef223456" eId="hauptteil-1_art-3_abs-1_inhalt-1">
                        <akn:p GUID="c5d6e7f8-a9b0-4890-abcd-4567ef223456" eId="hauptteil-1_art-3_abs-1_inhalt-1_text-1">Test.</akn:p>
                    </akn:content>
                </akn:paragraph>
                <akn:paragraph GUID="d6e7f8a9-b0c2-4890-abcd-5678ef123456" eId="hauptteil-1_art-3_abs-2">
                    <akn:num GUID="e7f8a9b0-c2d2-4890-abcd-6789ef123456" eId="hauptteil-1_art-3_abs-2_bezeichnung-1">(2)</akn:num>
                    <akn:content GUID="f8a9b0c2-d2e3-4890-abcd-7890ef123456" eId="hauptteil-1_art-3_abs-2_inhalt-1">
                        <akn:p GUID="a9b0c2d2-e3f4-4890-abcd-8901ef123456" eId="hauptteil-1_art-3_abs-2_inhalt-1_text-1">Das Bundesministerium des Innern, für Bau und Heimat kündigt den Zeitpunkt der Umsetzung an.</akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
        </akn:body>
