        <akn:preamble GUID="d1e2f3a4-b5c6-4789-abcd-234567890abc" eId="preambel-1">Fiktiver Prologstext zur Einführung in das Gesetz.</akn:preamble>
        <akn:body GUID="e1f2a3b4-c5d6-4789-abcd-34567890abcd" eId="hauptteil-1">
            <akn:article GUID="f1a2b3c4-d5e6-4789-abcd-4567890abcde" eId="hauptteil-1_art-1" period="#meta-1_geltzeiten-1_geltungszeitgr-1" refersTo="stammform">
                <akn:num GUID="a2b3c4d5-e6f7-4789-abcd-567890abcdef" eId="hauptteil-1_art-1_bezeichnung-1">§ 1</akn:num>
                <akn:heading GUID="b3c4d5e6-f7a8-4789-abcd-67890abcdef1" eId="hauptteil-1_art-1_überschrift-1">Beispielhafte Illustration</akn:heading>
                <akn:paragraph GUID="c4d5e6f7-a8b9-4789-abcd-7890abcdef12" eId="hauptteil-1_art-1_abs-1">
                    <akn:num GUID="d5e6f7a8-b9ca-4789-abcd-890abcdef123" eId="hauptteil-1_art-1_abs-1_bezeichnung-1">(1)</akn:num>
                    <akn:content GUID="e6f7a8b9-cad0-4789-abcd-901abcdef234" eId="hauptteil-1_art-1_abs-1_inhalt-1">
                        <akn:p GUID="f7a8b9ca-d0e1-4789-abcd-0123abcdef34" eId="hauptteil-1_art-1_abs-1_inhalt-1_text-1">Fiktive Einrichtungen im öffentlichen Interesse werden von den zuständigen fiktiven Trägern anerkannt.</akn:p>
                    </akn:content>
                </akn:paragraph>
                <akn:paragraph GUID="a8b9cad0-e1f2-4789-abcd-1234abcdef45" eId="hauptteil-1_art-1_abs-2">
                    <akn:num GUID="b9cad0e1-f2a3-4789-abcd-2345abcdef56" eId="hauptteil-1_art-1_abs-2_bezeichnung-1">(2)</akn:num>
                    <akn:content GUID="cad0e1f2-a3b4-4789-abcd-3456abcdef67" eId="hauptteil-1_art-1_abs-2_inhalt-1">
                        <akn:p GUID="dae1f2a3-b4c5-4789-abcd-4567abcdef78" eId="hauptteil-1_art-1_abs-2_inhalt-1_text-1">
                            Beispielhafte Illustration
                            <akn:img GUID="7f4172f8-2184-4b13-98b3-d387b6b25a72"
                                     eId="hauptteil-1_art-1_abs-2_inhalt-1_bild-1"
                                     src="eli/bund/bgbl-1/2024/383/2024-12-19/1/deu/2024-12-19/illustration.png" alt="Beispielbild"/></akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
            <akn:article GUID="e2f3a4b5-c6d7-4789-abcd-5678abcdef89" eId="hauptteil-1_art-2" period="#meta-1_geltzeiten-1_geltungszeitgr-1" refersTo="stammform">
                <akn:num GUID="f3a4b5c6-d7e8-4789-abcd-6789abcdef90" eId="hauptteil-1_art-2_bezeichnung-1">§ 2</akn:num>
                <akn:heading GUID="a4b5c6d7-e8f9-4789-abcd-789abcdef012" eId="hauptteil-1_art-2_überschrift-1"/>
                <akn:paragraph GUID="b5c6d7e8-f9a0-4789-abcd-89abcdef0123" eId="hauptteil-1_art-2_abs-1">
                    <akn:num GUID="c6d7e8f9-a0b1-4789-abcd-90abcdef1234" eId="hauptteil-1_art-2_abs-1_bezeichnung-1">(1)</akn:num>
                    <akn:content GUID="d7e8f9a0-b1c2-4789-abcd-01abcdef2345" eId="hauptteil-1_art-2_abs-1_inhalt-1">
                        <akn:p GUID="e8f9a0b1-c2d3-4789-abcd-12abcdef3456" eId="hauptteil-1_art-2_abs-1_inhalt-1_text-1">Dieses Gesetz tritt am Tag nach der Verkündung in Kraft.</akn:p>
                    </akn:content>
                </akn:paragraph>
                <akn:paragraph GUID="f9a0b1c2-d3e4-4789-abcd-23abcdef4567" eId="hauptteil-1_art-2_abs-2">
                    <akn:num GUID="a0b1c2d3-e4f5-4789-abcd-34abcdef5678" eId="hauptteil-1_art-2_abs-2_bezeichnung-1">(2)</akn:num>
                    <akn:content GUID="b1c2d3e4-f5a6-4789-abcd-45abcdef6789" eId="hauptteil-1_art-2_abs-2_inhalt-1">
                        <akn:p GUID="c2d3e4f5-a6b7-4789-abcd-56abcdef7890" eId="hauptteil-1_art-2_abs-2_inhalt-1_text-1">Das zuständige Ministerium kündigt den Umsetzungstermin an.</akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
        </akn:body>
    </akn:act>
