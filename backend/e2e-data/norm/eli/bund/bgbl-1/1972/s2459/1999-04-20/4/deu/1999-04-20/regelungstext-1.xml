                    <akn:docTitle GUID="66666666-7777-8888-9999-bbbbbbbbbbbb" eId="einleitung-1_doktitel-1_text-1_doctitel-1">
                        Musterverordnung über den Einsatz von Wahlgeräten im Rahmen der Bundestagswahl
                    </akn:docTitle>
                    <akn:shortTitle GUID="77777777-8888-9999-aaaa-cccccccccccc" eId="einleitung-1_doktitel-1_text-1_kurztitel-1">
                        (Bundeswahlgeräteverordnung - <akn:inline GUID="88888888-9999-aaaa-bbbb-dddddddddddd" eId="einleitung-1_doktitel-1_text-1_kurztitel-1_inline-1" name="attributsemantik-noch-undefiniert" refersTo="amtliche-abkuerzung">BWahlGV</akn:inline>)
                    </akn:shortTitle>
                </akn:p>
            </akn:longTitle>
        </akn:preface>
        <akn:preamble GUID="99999999-aaaa-bbbb-cccc-dddddddddddd" eId="preambel-1">
            <akn:formula GUID="aaaaaaaa-bbbb-cccc-dddd-eeeeeeeeeeee" eId="preambel-1_formel-1" name="attributsemantik-noch-undefiniert" refersTo="eingangsformel">
                <akn:p GUID="bbbbbbbb-cccc-dddd-eeee-ffffffffffff" eId="preambel-1_formel-1_text-1">
                    Aufgrund der gesetzlichen Grundlagen erfolgt die Bekanntmachung dieser Verordnung zur Verwendung von Wahlgeräten.
                </akn:p>
            </akn:formula>
        </akn:preamble>
        <akn:body GUID="cccccccc-dddd-eeee-ffff-aaaaaaaaaaaa" eId="hauptteil-1">
            <akn:section GUID="dddddddd-eeee-ffff-0000-bbbbbbbbbbbb" eId="hauptteil-1_abschnitt-1">
                <akn:num GUID="eeeeeeee-ffff-0000-1111-cccccccccccc" eId="hauptteil-1_abschnitt-1_bezeichnung-1">
                    Erster Abschnitt
                </akn:num>
                <akn:heading GUID="ffffffff-0000-1111-2222-dddddddddddd" eId="hauptteil-1_abschnitt-1_überschrift-1">
                    Zulassung und Regelung des Einsatzes von Wahlgeräten
                </akn:heading>
                <akn:article GUID="11111111-2222-3333-4444-777777777777" eId="hauptteil-1_abschnitt-1_art-1" period="#meta-1_geltzeiten-1_geltungszeitgr-3" refersTo="stammform">
                    <akn:num GUID="22222222-3333-4444-5555-888888888888" eId="hauptteil-1_abschnitt-1_art-1_bezeichnung-1">§ 1</akn:num>
                    <akn:heading GUID="33333333-4444-5555-6666-999999999999" eId="hauptteil-1_abschnitt-1_art-1_überschrift-1">
                        Zulassungspflicht und Bauartzulassung
                    </akn:heading>
                    <akn:paragraph GUID="44444444-5555-6666-7777-aaaaaaaaaaaa" eId="hauptteil-1_abschnitt-1_art-1_abs-1">
                        <akn:num GUID="55555555-6666-7777-8888-bbbbbbbbbbbb" eId="hauptteil-1_abschnitt-1_art-1_abs-1_bezeichnung-1">(1)</akn:num>
                        <akn:content GUID="66666666-7777-8888-9999-cccccccccccc" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1">
                            <akn:p GUID="77777777-8888-9999-aaaa-dddddddddddd" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_text-1">
                                Wahlgeräte müssen vor ihrer Verwendung gemäß den geltenden technischen Normen zugelassen sein.
                            </akn:p>
                            <akn:blockList GUID="88888888-9999-aaaa-bbbb-eeeeeeeeeeee" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_liste-1">
                                <akn:item GUID="99999999-aaaa-bbbb-cccc-ffffffffffff" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_liste-1_listenelem-1">
                                    <akn:num GUID="aaaaaaaa-bbbb-cccc-dddd-111111111111" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_liste-1_listenelem-1_bezeichnung-1">1.</akn:num>
                                    <akn:p GUID="bbbbbbbb-cccc-dddd-eeee-222222222222" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_liste-1_listenelem-1_text-1">
                                        Die Zulassung umfasst eine Prüfung der technischen Ausstattung,
                                    </akn:p>
                                </akn:item>
                                <akn:item GUID="cccccccc-dddd-eeee-ffff-333333333333" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_liste-1_listenelem-2">
                                    <akn:num GUID="dddddddd-eeee-ffff-0000-444444444444" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_liste-1_listenelem-2_bezeichnung-1">2.</akn:num>
                                    <akn:p GUID="eeeeeeee-ffff-0000-1111-555555555555" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_liste-1_listenelem-2_text-1">
                                        eine technische Überprüfung der elektronischen Komponenten.
                                    </akn:p>
                                </akn:item>
                            </akn:blockList>
                        </akn:content>
                    </akn:paragraph>
                    <akn:paragraph GUID="ffffffff-0000-1111-2222-666666666666" eId="hauptteil-1_abschnitt-1_art-1_abs-2">
                        <akn:num GUID="11111111-2222-3333-4444-777777777778" eId="hauptteil-1_abschnitt-1_art-1_abs-2_bezeichnung-1">(2)</akn:num>
                        <akn:content GUID="22222222-3333-4444-5555-888888888889" eId="hauptteil-1_abschnitt-1_art-1_abs-2_inhalt-1">
                            <akn:p GUID="33333333-4444-5555-6666-999999999998" eId="hauptteil-1_abschnitt-1_art-1_abs-2_inhalt-1_text-1">
                                Die Zulassung wird durch das Bundesministerium des Innern kommuniziert.
                            </akn:p>
                        </akn:content>
                    </akn:paragraph>
                </akn:article>
            </akn:section>
            <akn:section GUID="44444444-5555-6666-7777-aaaaaaaaaabb" eId="hauptteil-1_abschnitt-2">
                <akn:num GUID="55555555-6666-7777-8888-bbbbbbbbbbbc" eId="hauptteil-1_abschnitt-2_bezeichnung-1">
                    Zweiter Abschnitt
                </akn:num>
                <akn:heading GUID="66666666-7777-8888-9999-cccccccccccd" eId="hauptteil-1_abschnitt-2_überschrift-1">
                    Durchführung der Wahl und Geräteüberwachung
                </akn:heading>
                <akn:article GUID="77777777-8888-9999-aaaa-dddddddddddd" eId="hauptteil-1_abschnitt-2_art-1" period="#meta-1_geltzeiten-1_geltungszeitgr-3" refersTo="stammform">
                    <akn:num GUID="88888888-9999-aaaa-bbbb-eeeeeeeeeeef" eId="hauptteil-1_abschnitt-2_art-1_bezeichnung-1">§ 5</akn:num>
                    <akn:heading GUID="99999999-aaaa-bbbb-cccc-ffffffffffff" eId="hauptteil-1_abschnitt-2_art-1_überschrift-1">
                        Geräteüberwachung und Einsatzkontrolle
                    </akn:heading>
                    <akn:paragraph GUID="aaaaaaaa-bbbb-cccc-dddd-111111111112" eId="hauptteil-1_abschnitt-2_art-1_abs-1">
                        <akn:num GUID="bbbbbbbb-cccc-dddd-eeee-222222222223" eId="hauptteil-1_abschnitt-2_art-1_abs-1_bezeichnung-1">(1)</akn:num>
                        <akn:content GUID="cccccccc-dddd-eeee-ffff-333333333334" eId="hauptteil-1_abschnitt-2_art-1_abs-1_inhalt-1">
                            <akn:p GUID="dddddddd-eeee-ffff-0000-444444444445" eId="hauptteil-1_abschnitt-2_art-1_abs-1_inhalt-1_text-1">
                                Die ordnungsgemäße Funktion der Wahlgeräte wird durch regelmäßige Prüfungen sichergestellt.
                            </akn:p>
                        </akn:content>
                    </akn:paragraph>
                </akn:article>
            </akn:section>
        </akn:body>
    </akn:act>
