        <akn:preface GUID="55555555-6666-7777-8888-aaaaaaaaaaaa" eId="einleitung-1">
            <akn:longTitle GUID="66666666-7777-8888-9999-bbbbbbbbbbbb" eId="einleitung-1_doktitel-1">
                <akn:p GUID="77777777-8888-9999-aaaa-cccccccccccc" eId="einleitung-1_doktitel-1_text-1">
                    <akn:docStage GUID="88888888-9999-aaaa-bbbb-dddddddddddd" eId="einleitung-1_doktitel-1_text-1_docstadium-1"/>
                    <akn:docTitle GUID="99999999-aaaa-bbbb-cccc-eeeeeeeeeeee" eId="einleitung-1_doktitel-1_text-1_doctitel-1">
                        Musterverordnung zur Testregelung im Bereich der Wahlen mit Wahlgeräten
                    </akn:docTitle>
                    <akn:shortTitle GUID="aaaaaaaa-bbbb-cccc-dddd-ffffffffffff" eId="einleitung-1_doktitel-1_text-1_kurztitel-1">
                        (Bundeswahlgeräteverordnung - <akn:inline GUID="bbbbbbbb-cccc-dddd-eeee-111111111111" eId="einleitung-1_doktitel-1_text-1_kurztitel-1_inline-1"
                                                                  name="attributsemantik-noch-undefiniert" refersTo="amtliche-abkuerzung">BWahlGV</akn:inline>)
                    </akn:shortTitle>
                </akn:p>
            </akn:longTitle>
        </akn:preface>
        <akn:preamble GUID="cccccccc-dddd-eeee-ffff-111111111111" eId="preambel-1">
            <akn:formula GUID="dddddddd-eeee-ffff-0000-222222222222" eId="preambel-1_formel-1" name="attributsemantik-noch-undefiniert"
                         refersTo="eingangsformel">
                <akn:p GUID="eeeeeeee-ffff-0000-1111-333333333333" eId="preambel-1_formel-1_text-1">
                    Im Rahmen der gesetzlichen Bestimmungen wird diese Testverordnung zur Erprobung des Einsatzes von Wahlgeräten bekannt gemacht.
                </akn:p>
            </akn:formula>
        </akn:preamble>
        <akn:body GUID="ffffffff-0000-1111-2222-444444444444" eId="hauptteil-1">
            <akn:article GUID="11111111-2222-3333-4444-555555555555" eId="hauptteil-1_art-1" period="#meta-1_geltzeiten-1_geltungszeitgr-2" refersTo="stammform">
                <akn:num GUID="22222222-3333-4444-5555-666666666666" eId="hauptteil-1_art-1_bezeichnung-1">§ 1</akn:num>
                <akn:heading GUID="33333333-4444-5555-6666-777777777777" eId="hauptteil-1_art-1_überschrift-1">
                    Zulassungspflicht für Wahlgeräte
                </akn:heading>
                <akn:paragraph GUID="44444444-5555-6666-7777-888888888888" eId="hauptteil-1_art-1_abs-1">
                    <akn:num GUID="55555555-6666-7777-8888-999999999999" eId="hauptteil-1_art-1_abs-1_bezeichnung-1">(1)</akn:num>
                    <akn:content GUID="66666666-7777-8888-9999-aaaaaaaaaaaa" eId="hauptteil-1_art-1_abs-1_inhalt-1">
                        <akn:p GUID="77777777-8888-9999-aaaa-bbbbbbbbbbbb" eId="hauptteil-1_art-1_abs-1_inhalt-1_text-1">
                            Zur Gewährleistung der ordnungsgemäßen Durchführung von Wahlen müssen alle Wahlgeräte vor ihrer Inbetriebnahme einer technischen Prüfung unterzogen werden.
                        </akn:p>
                        <akn:blockList GUID="88888888-9999-aaaa-bbbb-cccccccccccc" eId="hauptteil-1_art-1_abs-1_inhalt-1_liste-1">
                            <akn:item GUID="99999999-aaaa-bbbb-cccc-dddddddddddd" eId="hauptteil-1_art-1_abs-1_inhalt-1_liste-1_listenelem-1">
                                <akn:num GUID="aaaaaaaa-bbbb-cccc-dddd-111111111111" eId="hauptteil-1_art-1_abs-1_inhalt-1_liste-1_listenelem-1_bezeichnung-1">1.</akn:num>
                                <akn:p GUID="bbbbbbbb-cccc-dddd-eeee-222222222222" eId="hauptteil-1_art-1_abs-1_inhalt-1_liste-1_listenelem-1_text-1">
                                    Die Gerätebauart muss den aktuellen technischen Normen entsprechen,
                                </akn:p>
                            </akn:item>
                            <akn:item GUID="cccccccc-dddd-eeee-ffff-333333333333" eId="hauptteil-1_art-1_abs-1_inhalt-1_liste-1_listenelem-2">
                                <akn:num GUID="dddddddd-eeee-ffff-0000-444444444444" eId="hauptteil-1_art-1_abs-1_inhalt-1_liste-1_listenelem-2_bezeichnung-1">2.</akn:num>
                                <akn:p GUID="eeeeeeee-ffff-0000-1111-555555555555" eId="hauptteil-1_art-1_abs-1_inhalt-1_liste-1_listenelem-2_text-1">
                                    der Hersteller hat ein detailliertes Prüfzertifikat einzureichen.
                                </akn:p>
                            </akn:item>
                        </akn:blockList>
                    </akn:content>
                </akn:paragraph>
                <akn:paragraph GUID="ffffffff-0000-1111-2222-666666666666" eId="hauptteil-1_art-1_abs-2">
                    <akn:num GUID="11111111-2222-3333-4444-777777777777" eId="hauptteil-1_art-1_abs-2_bezeichnung-1">(2)</akn:num>
                    <akn:content GUID="22222222-3333-4444-5555-888888888888" eId="hauptteil-1_art-1_abs-2_inhalt-1">
                        <akn:p GUID="33333333-4444-5555-6666-999999999999" eId="hauptteil-1_art-1_abs-2_inhalt-1_text-1">
                            Die Zulassungsentscheidung wird vom Bundesministerium des Innern bekannt gegeben.
                        </akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
            <akn:article GUID="44444444-5555-6666-7777-aaaaaaaaaaaa" eId="hauptteil-1_art-2" period="#meta-1_geltzeiten-1_geltungszeitgr-4" refersTo="stammform">
                <akn:num GUID="55555555-6666-7777-8888-bbbbbbbbbbbb" eId="hauptteil-1_art-2_bezeichnung-1">§ 2</akn:num>
                <akn:heading GUID="66666666-7777-8888-9999-cccccccccccc" eId="hauptteil-1_art-2_überschrift-1">
                    Geräteüberwachung
                </akn:heading>
                <akn:paragraph GUID="77777777-8888-9999-aaaa-dddddddddddd" eId="hauptteil-1_art-2_abs-1">
                    <akn:num GUID="88888888-9999-aaaa-bbbb-eeeeeeeeeeee" eId="hauptteil-1_art-2_abs-1_bezeichnung-1">(1)</akn:num>
                    <akn:content GUID="99999999-aaaa-bbbb-cccc-ffffffffffff" eId="hauptteil-1_art-2_abs-1_inhalt-1">
                        <akn:p GUID="aaaaaaaa-bbbb-cccc-dddd-111111111111" eId="hauptteil-1_art-2_abs-1_inhalt-1_text-1">
                            Die Funktionstüchtigkeit der Wahlgeräte wird in regelmäßigen Intervallen kontrolliert.
                        </akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
        </akn:body>
        <akn:conclusions GUID="bbbbbbbb-cccc-dddd-eeee-222222222222" eId="schluss-1">
