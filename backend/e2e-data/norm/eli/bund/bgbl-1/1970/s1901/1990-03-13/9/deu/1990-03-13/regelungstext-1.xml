        <akn:body GUID="11111111-2222-3333-4444-555555555555" eId="hauptteil-1">
            <akn:section GUID="22222222-3333-4444-5555-666666666666" eId="hauptteil-1_abschnitt-1">
                <akn:num GUID="33333333-4444-5555-6666-777777777777" eId="hauptteil-1_abschnitt-1_bezeichnung-1">I. Abschnitt</akn:num>
                <akn:heading GUID="44444444-5555-6666-7777-888888888888" eId="hauptteil-1_abschnitt-1_überschrift-1">Erholungsurlaub</akn:heading>
                <akn:article GUID="55555555-6666-7777-8888-999999999999" eId="hauptteil-1_abschnitt-1_art-1" period="#meta-1_geltzeiten-1_geltungszeitgr-3" refersTo="stammform">
                    <akn:num GUID="66666666-7777-8888-9999-aaaaaaaaaaaa" eId="hauptteil-1_abschnitt-1_art-1_bezeichnung-1">§ 1</akn:num>
                    <akn:heading GUID="77777777-8888-9999-aaaa-bbbbbbbbbbbb" eId="hauptteil-1_abschnitt-1_art-1_überschrift-1">Anwendung der Inlandsregelungen im Erholungsurlaub</akn:heading>
                    <akn:paragraph GUID="88888888-9999-aaaa-bbbb-cccccccccccc" eId="hauptteil-1_abschnitt-1_art-1_abs-1">
                        <akn:num GUID="99999999-aaaa-bbbb-cccc-dddddddddddd" eId="hauptteil-1_abschnitt-1_art-1_abs-1_bezeichnung-1">(1)</akn:num>
                        <akn:content GUID="aaaaaaaa-bbbb-cccc-dddd-eeeeeeeeeeee" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1">
                            <akn:p GUID="bbbbbbbb-cccc-dddd-eeee-ffffffffffff" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_text-1">Für den Erholungsurlaub im internationalen Dienst ist eine flexible Freistellung mit fortlaufender Bezahlung zu gewähren.</akn:p>
                            <akn:blockList GUID="cccccccc-dddd-eeee-ffff-000000000000" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_liste-1">
                                <akn:item GUID="dddddddd-eeee-ffff-0000-111111111112" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_liste-1_listenelem-1">
                                    <akn:num GUID="eeeeeeee-ffff-0000-1111-222222222223" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_liste-1_listenelem-1_bezeichnung-1">1.</akn:num>
                                    <akn:p GUID="ffffffff-0000-1111-2222-333333333334" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_liste-1_listenelem-1_text-1">zur Teilnahme an regionalen Gesundheitsprojekten,</akn:p>
                                </akn:item>
                                <akn:item GUID="11111111-0000-2222-3333-444444444444" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_liste-1_listenelem-2">
                                    <akn:num GUID="22222222-0000-3333-4444-555555555555" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_liste-1_listenelem-2_bezeichnung-1">2.</akn:num>
                                    <akn:p GUID="33333333-0000-4444-5555-666666666666" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_liste-1_listenelem-2_text-1">zur Erprobung von neuen Dienstmodellen,</akn:p>
                                </akn:item>
                                <akn:item GUID="44444444-0000-5555-6666-777777777777" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_liste-1_listenelem-3">
                                    <akn:num GUID="55555555-0000-6666-7777-888888888888" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_liste-1_listenelem-3_bezeichnung-1">3.</akn:num>
                                    <akn:p GUID="66666666-0000-7777-8888-999999999999" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_liste-1_listenelem-3_text-1">zur Durchführung innovativer Testverfahren.</akn:p>
                                </akn:item>
                            </akn:blockList>
                        </akn:content>
                    </akn:paragraph>
                    <akn:paragraph GUID="77777777-8888-9999-aaaa-bbbbbbbbbbbc" eId="hauptteil-1_abschnitt-1_art-1_abs-2">
                        <akn:num GUID="88888888-9999-aaaa-bbbb-cccccccccccd" eId="hauptteil-1_abschnitt-1_art-1_abs-2_bezeichnung-1">(2)</akn:num>
                        <akn:content GUID="99999999-aaaa-bbbb-cccc-ddddddddddd" eId="hauptteil-1_abschnitt-1_art-1_abs-2_inhalt-1">
                            <akn:p GUID="aaaaaaaa-bbbb-cccc-dddd-eeeeeeeeeeef" eId="hauptteil-1_abschnitt-1_art-1_abs-2_inhalt-1_text-1">Bei sich ändernden internationalen Rahmenbedingungen erfolgt eine regelmäßige Überprüfung der Freistellungsmodalitäten.</akn:p>
                        </akn:content>
                    </akn:paragraph>
                </akn:article>
            </akn:section>
            <akn:section GUID="aaaaaaaa-bbbb-cccc-dddd-eeeeeeeeeeef" eId="hauptteil-1_abschnitt-2">
                <akn:num GUID="bbbbbbbb-cccc-dddd-eeee-ffffffffffff" eId="hauptteil-1_abschnitt-2_bezeichnung-1">II. Abschnitt</akn:num>
                <akn:heading GUID="cccccccc-dddd-eeee-ffff-000000000001" eId="hauptteil-1_abschnitt-2_überschrift-1">Heimaturlaub</akn:heading>
                <akn:article GUID="dddddddd-eeee-ffff-0000-111111111113" eId="hauptteil-1_abschnitt-2_art-1" period="#meta-1_geltzeiten-1_geltungszeitgr-4" refersTo="stammform">
                    <akn:num GUID="eeeeeeee-ffff-0000-1111-222222222224" eId="hauptteil-1_abschnitt-2_art-1_bezeichnung-1">§ 4</akn:num>
                    <akn:heading GUID="ffffffff-0000-1111-2222-333333333335" eId="hauptteil-1_abschnitt-2_art-1_überschrift-1">Heimaturlaubsberechtigung</akn:heading>
                    <akn:paragraph GUID="11111111-2222-3333-4444-555555555556" eId="hauptteil-1_abschnitt-2_art-1_abs-1">
                        <akn:num GUID="22222222-3333-4444-5555-666666666667" eId="hauptteil-1_abschnitt-2_art-1_abs-1_bezeichnung-1">(1)</akn:num>
                        <akn:content GUID="33333333-4444-5555-6666-777777777778" eId="hauptteil-1_abschnitt-2_art-1_abs-1_inhalt-1">
                            <akn:p GUID="44444444-5555-6666-7777-888888888889" eId="hauptteil-1_abschnitt-2_art-1_abs-1_inhalt-1_text-1">Für im Ausland eingesetzte Beamte wird Heimaturlaub auf Antrag gewährt – unter Berücksichtigung besonderer internationaler Regelungen.</akn:p>
                            <akn:blockList GUID="55555555-6666-7777-8888-999999999990" eId="hauptteil-1_abschnitt-2_art-1_abs-1_inhalt-1_liste-1">
                                <akn:item GUID="66666666-7777-8888-9999-aaaaaaaaaaab" eId="hauptteil-1_abschnitt-2_art-1_abs-1_inhalt-1_liste-1_listenelem-1">
                                    <akn:num GUID="77777777-8888-9999-aaaa-bbbbbbbbbbbb" eId="hauptteil-1_abschnitt-2_art-1_abs-1_inhalt-1_liste-1_listenelem-1_bezeichnung-1">1.</akn:num>
                                    <akn:p GUID="88888888-9999-aaaa-bbbb-cccccccccccc" eId="hauptteil-1_abschnitt-2_art-1_abs-1_inhalt-1_liste-1_listenelem-1_text-1">für den internationalen Personalaustausch,</akn:p>
                                </akn:item>
                                <akn:item GUID="99999999-aaaa-bbbb-cccc-dddddddddddd" eId="hauptteil-1_abschnitt-2_art-1_abs-1_inhalt-1_liste-1_listenelem-2">
                                    <akn:num GUID="aaaaaaaa-bbbb-cccc-dddd-eeeeeeeeeeee" eId="hauptteil-1_abschnitt-2_art-1_abs-1_inhalt-1_liste-1_listenelem-2_bezeichnung-1">2.</akn:num>
                                    <akn:p GUID="bbbbbbbb-cccc-dddd-eeee-ffffffffffff" eId="hauptteil-1_abschnitt-2_art-1_abs-1_inhalt-1_liste-1_listenelem-2_text-1">zur Durchführung internationaler Fortbildungen,</akn:p>
                                </akn:item>
                                <akn:item GUID="cccccccc-dddd-eeee-ffff-000000000000" eId="hauptteil-1_abschnitt-2_art-1_abs-1_inhalt-1_liste-1_listenelem-3">
                                    <akn:num GUID="dddddddd-eeee-ffff-0000-111111111111" eId="hauptteil-1_abschnitt-2_art-1_abs-1_inhalt-1_liste-1_listenelem-3_bezeichnung-1">3.</akn:num>
                                    <akn:p GUID="eeeeeeee-ffff-0000-1111-222222222222" eId="hauptteil-1_abschnitt-2_art-1_abs-1_inhalt-1_liste-1_listenelem-3_text-1">zur Förderung des interkulturellen Austauschs.</akn:p>
                                </akn:item>
                            </akn:blockList>
                        </akn:content>
                    </akn:paragraph>
                    <akn:paragraph GUID="ffffffff-0000-1111-2222-333333333336" eId="hauptteil-1_abschnitt-2_art-1_abs-2">
                        <akn:num GUID="11111111-2222-3333-4444-555555555557" eId="hauptteil-1_abschnitt-2_art-1_abs-2_bezeichnung-1">(2)</akn:num>
                        <akn:content GUID="22222222-3333-4444-5555-666666666668" eId="hauptteil-1_abschnitt-2_art-1_abs-2_inhalt-1">
                            <akn:p GUID="33333333-4444-5555-6666-777777777779" eId="hauptteil-1_abschnitt-2_art-1_abs-2_inhalt-1_text-1">Spezielle dienstliche Erfordernisse können zu einer individuellen Anpassung des Heimaturlaubs führen.</akn:p>
                        </akn:content>
                    </akn:paragraph>
                </akn:article>
            </akn:section>
        </akn:body>
    </akn:act>
</akn:akomaNtoso>
