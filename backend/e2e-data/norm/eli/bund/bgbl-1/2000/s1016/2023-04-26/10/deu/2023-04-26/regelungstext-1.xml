            <akn:formula GUID="d9999999-9999-9999-9999-999999999999" eId="preambel-1_formel-1" name="attributsemantik-noch-undefiniert" refersTo="eingangsformel">
                <akn:p GUID="da111111-1111-1111-1111-111111111111" eId="preambel-1_formel-1_text-1">
                    Auf Grundlage des fiktiven Lebensmittel- und Bedarfsgegenständegesetzes in der Fassung vom 27. Mai 2000 und unter Einbeziehung aktueller Testdaten wird diese Verordnung erlassen:
                </akn:p>
                <akn:blockList GUID="da222222-2222-2222-2222-222222222222" eId="preambel-1_formel-1_liste-1">
                    <akn:item GUID="da333333-3333-3333-3333-333333333333" eId="preambel-1_formel-1_liste-1_listenelem-1">
                        <akn:num GUID="da444444-4444-4444-4444-444444444444" eId="preambel-1_formel-1_liste-1_listenelem-1_bezeichnung-1">-</akn:num>
                        <akn:p GUID="da555555-5555-5555-5555-555555555555" eId="preambel-1_formel-1_liste-1_listenelem-1_text-1">
                            auf Grund des § 12 Abs. 1 Nr. 1 und Abs. 2 Nr. 2a und 3 in Verbindung mit Abs. 3 des fiktiven Lebensmittelgesetzes
                        </akn:p>
                    </akn:item>
                    <akn:item GUID="da666666-6666-6666-6666-666666666666" eId="preambel-1_formel-1_liste-1_listenelem-2">
                        <akn:num GUID="da777777-7777-7777-7777-777777777777" eId="preambel-1_formel-1_liste-1_listenelem-2_bezeichnung-1">-</akn:num>
                        <akn:p GUID="da888888-8888-8888-8888-888888888888" eId="preambel-1_formel-1_liste-1_listenelem-2_text-1">
                            unter Einvernehmen mit den zuständigen Ministerien
                        </akn:p>
                    </akn:item>
                </akn:blockList>
                <akn:p GUID="da999999-9999-9999-9999-999999999999" eId="preambel-1_formel-1_text-2">
                    (Letzte Änderung durch fiktive Testanpassungen)
                </akn:p>
            </akn:formula>
        </akn:preamble>
        <akn:body GUID="db111111-1111-1111-1111-111111111111" eId="hauptteil-1">
            <akn:section GUID="db222222-2222-2222-2222-222222222222" eId="hauptteil-1_abschnitt-1">
                <akn:num GUID="db333333-3333-3333-3333-333333333333" eId="hauptteil-1_abschnitt-1_bezeichnung-1">
                    Abschnitt 1
                </akn:num>
                <akn:heading GUID="db444444-4444-4444-4444-444444444444" eId="hauptteil-1_abschnitt-1_überschrift-1">
                    Anwendungsbereich
                </akn:heading>
                <akn:article GUID="db555555-5555-5555-5555-555555555555" eId="hauptteil-1_abschnitt-1_art-1" period="#meta-1_geltzeiten-1_geltungszeitgr-26" refersTo="stammform">
                    <akn:num GUID="db666666-6666-6666-6666-666666666666" eId="hauptteil-1_abschnitt-1_art-1_bezeichnung-1">
                        § 1
                    </akn:num>
                    <akn:heading GUID="db777777-7777-7777-7777-777777777777" eId="hauptteil-1_abschnitt-1_art-1_überschrift-1">
                        Anwendungsbereich
                    </akn:heading>
                    <akn:paragraph GUID="db888888-8888-8888-8888-888888888888" eId="hauptteil-1_abschnitt-1_art-1_abs-1">
                        <akn:num GUID="db999999-9999-9999-9999-999999999999" eId="hauptteil-1_abschnitt-1_art-1_abs-1_bezeichnung-1">(1)</akn:num>
                        <akn:content GUID="dc111111-1111-1111-1111-111111111111" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1">
                            <akn:p GUID="dc222222-2222-2222-2222-222222222222" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_text-1">
                                Die in Anlage 1 aufgeführten Erzeugnisse unterliegen dieser Verordnung, soweit sie zum gewerbsmäßigen Vertrieb bestimmt sind.
                                <akn:marker GUID="dc333333-3333-3333-3333-333333333333" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_text-1_marker-1" refersTo="satzende"/>
                            </akn:p>
                        </akn:content>
                    </akn:paragraph>
                    <akn:paragraph GUID="dc444444-4444-4444-4444-444444444444" eId="hauptteil-1_abschnitt-1_art-1_abs-2">
                        <akn:num GUID="dc555555-5555-5555-5555-555555555555" eId="hauptteil-1_abschnitt-1_art-1_abs-2_bezeichnung-1">(2)</akn:num>
                        <akn:content GUID="dc666666-6666-6666-6666-666666666666" eId="hauptteil-1_abschnitt-1_art-1_abs-2_inhalt-1">
                            <akn:p GUID="dc777777-7777-7777-7777-777777777777" eId="hauptteil-1_abschnitt-1_art-1_abs-2_inhalt-1_text-1">
                                Diese Verordnung gilt ergänzend auch für Produkte, die in den Anwendungsbereich des Abschnitts 3 fallen.
                                <akn:marker GUID="dc888888-8888-8888-8888-888888888888" eId="hauptteil-1_abschnitt-1_art-1_abs-2_inhalt-1_text-1_marker-1" refersTo="satzende"/>
                            </akn:p>
                        </akn:content>
                    </akn:paragraph>
                </akn:article>
            </akn:section>
            <akn:section GUID="dd111111-1111-1111-1111-111111111111" eId="hauptteil-1_abschnitt-2">
                <akn:num GUID="dd222222-2222-2222-2222-222222222222" eId="hauptteil-1_abschnitt-2_bezeichnung-1">
                    Abschnitt 2
                </akn:num>
                <akn:heading GUID="dd333333-3333-3333-3333-333333333333" eId="hauptteil-1_abschnitt-2_überschrift-1">
                    Fruchtsaft, einige ähnliche Erzeugnisse und Fruchtnektar
                </akn:heading>
                <akn:article GUID="dd444444-4444-4444-4444-444444444444" eId="hauptteil-1_abschnitt-2_art-1" period="#meta-1_geltzeiten-1_geltungszeitgr-26" refersTo="stammform">
                    <akn:num GUID="dd555555-5555-5555-5555-555555555555" eId="hauptteil-1_abschnitt-2_art-1_bezeichnung-1">
                        § 2
                    </akn:num>
                    <akn:heading GUID="dd666666-6666-6666-6666-666666666666" eId="hauptteil-1_abschnitt-2_art-1_überschrift-1">
                        Zutaten, Herstellungsanforderungen
                    </akn:heading>
                    <akn:paragraph GUID="dd777777-7777-7777-7777-777777777777" eId="hauptteil-1_abschnitt-2_art-1_abs-1">
                        <akn:num GUID="dd888888-8888-8888-8888-888888888888" eId="hauptteil-1_abschnitt-2_art-1_abs-1_bezeichnung-1">(1)</akn:num>
                        <akn:content GUID="dd999999-9999-9999-9999-999999999999" eId="hauptteil-1_abschnitt-2_art-1_abs-1_inhalt-1">
                            <akn:p GUID="de111111-1111-1111-1111-111111111111" eId="hauptteil-1_abschnitt-2_art-1_abs-1_inhalt-1_text-1">
                                Die Ausgangserzeugnisse für Erzeugnisse gemäß Anlage 1 müssen den in Anlage 2 festgelegten Qualitätsstandards entsprechen.
                                <akn:marker GUID="de222222-2222-2222-2222-222222222222" eId="hauptteil-1_abschnitt-2_art-1_abs-1_inhalt-1_text-1_marker-1" refersTo="satzende"/>
                            </akn:p>
                        </akn:content>
                    </akn:paragraph>
                    <akn:paragraph GUID="de333333-3333-3333-3333-333333333333" eId="hauptteil-1_abschnitt-2_art-1_abs-2">
                        <akn:num GUID="de444444-4444-4444-4444-444444444444" eId="hauptteil-1_abschnitt-2_art-1_abs-2_bezeichnung-1">(2)</akn:num>
                        <akn:content GUID="de555555-5555-5555-5555-555555555555" eId="hauptteil-1_abschnitt-2_art-1_abs-2_inhalt-1">
                            <akn:p GUID="de666666-6666-6666-6666-666666666666" eId="hauptteil-1_abschnitt-2_art-1_abs-2_inhalt-1_text-1">
                                Für die Herstellung von Produkten gemäß Anlage 1 dürfen ausschließlich die in Anlage 3 genannten Zutaten verwendet werden.
                                <akn:marker GUID="de777777-7777-7777-7777-777777777777" eId="hauptteil-1_abschnitt-2_art-1_abs-2_inhalt-1_text-1_marker-1" refersTo="satzende"/>
                            </akn:p>
                        </akn:content>
                    </akn:paragraph>
                    <akn:paragraph GUID="de888888-8888-8888-8888-888888888888" eId="hauptteil-1_abschnitt-2_art-1_abs-3">
                        <akn:num GUID="de999999-9999-9999-9999-999999999999" eId="hauptteil-1_abschnitt-2_art-1_abs-3_bezeichnung-1">(3)</akn:num>
                        <akn:content GUID="df111111-1111-1111-1111-111111111111" eId="hauptteil-1_abschnitt-2_art-1_abs-3_inhalt-1">
                            <akn:p GUID="df222222-2222-2222-2222-222222222222" eId="hauptteil-1_abschnitt-2_art-1_abs-3_inhalt-1_text-1">
                                Bei der Anwendung der in Anlage 4 Abschnitt A genannten Verfahren sind die dort festgelegten technischen Vorgaben strikt einzuhalten.
                                <akn:marker GUID="df333333-3333-3333-3333-333333333333" eId="hauptteil-1_abschnitt-2_art-1_abs-3_inhalt-1_text-1_marker-1" refersTo="satzende"/>
                            </akn:p>
                        </akn:content>
                    </akn:paragraph>
                    <akn:paragraph GUID="df444444-4444-4444-4444-444444444444" eId="hauptteil-1_abschnitt-2_art-1_abs-4">
                        <akn:num GUID="df555555-5555-5555-5555-555555555555" eId="hauptteil-1_abschnitt-2_art-1_abs-4_bezeichnung-1">(4)</akn:num>
                        <akn:content GUID="df666666-6666-6666-6666-666666666666" eId="hauptteil-1_abschnitt-2_art-1_abs-4_inhalt-1">
                            <akn:p GUID="df777777-7777-7777-7777-777777777777" eId="hauptteil-1_abschnitt-2_art-1_abs-4_inhalt-1_text-1">
                                Beim gewerbsmäßigen Herstellen von Erzeugnissen nach Anlage 1 dürfen – vorbehaltlich Absatz 5 Satz 2 sowie Absatz 7 – keine anderen als die in den Absätzen 1 und 2 bezeichneten Zutaten verwendet werden.
                                <akn:marker GUID="df888888-8888-8888-8888-888888888888" eId="hauptteil-1_abschnitt-2_art-1_abs-4_inhalt-1_text-1_marker-1" refersTo="satzende"/>
                            </akn:p>
                            <akn:blockList GUID="df999999-9999-9999-9999-999999999999" eId="hauptteil-1_abschnitt-2_art-1_abs-4_inhalt-1_liste-1">
                                <akn:item GUID="e0111111-1111-1111-1111-111111111111" eId="hauptteil-1_abschnitt-2_art-1_abs-4_inhalt-1_liste-1_listenelem-1">
                                    <akn:num GUID="e0222222-2222-2222-2222-222222222222" eId="hauptteil-1_abschnitt-2_art-1_abs-4_inhalt-1_liste-1_listenelem-1_bezeichnung-1">
                                        1.
                                    </akn:num>
                                    <akn:p GUID="e0333333-3333-3333-3333-333333333333" eId="hauptteil-1_abschnitt-2_art-1_abs-4_inhalt-1_liste-1_listenelem-1_text-1">
                                        Zutaten, die ausdrücklich ausgeschlossen sind.
                                    </akn:p>
                                </akn:item>
                                <akn:item GUID="e0444444-4444-4444-4444-444444444444" eId="hauptteil-1_abschnitt-2_art-1_abs-4_inhalt-1_liste-1_listenelem-2">
                                    <akn:num GUID="e0555555-5555-5555-5555-555555555555" eId="hauptteil-1_abschnitt-2_art-1_abs-4_inhalt-1_liste-1_listenelem-2_bezeichnung-1">
                                        2.
                                    </akn:num>
                                    <akn:p GUID="e0666666-6666-6666-6666-666666666666" eId="hauptteil-1_abschnitt-2_art-1_abs-4_inhalt-1_liste-1_listenelem-2_text-1">
                                        Verfahren, die nicht angewendet werden dürfen.
                                    </akn:p>
                                </akn:item>
                            </akn:blockList>
                            <akn:p GUID="e0777777-7777-7777-7777-777777777777" eId="hauptteil-1_abschnitt-2_art-1_abs-4_inhalt-1_text-2">
                                werden.
                                <akn:marker GUID="e0888888-8888-8888-8888-888888888888" eId="hauptteil-1_abschnitt-2_art-1_abs-4_inhalt-1_text-2_marker-1" refersTo="satzende"/>
                            </akn:p>
                        </akn:content>
                    </akn:paragraph>
                    <akn:paragraph GUID="e0999999-9999-9999-9999-999999999999" eId="hauptteil-1_abschnitt-2_art-1_abs-5">
                        <akn:num GUID="ea111111-1111-1111-1111-111111111111" eId="hauptteil-1_abschnitt-2_art-1_abs-5_bezeichnung-1">(5)</akn:num>
                        <akn:content GUID="ea222222-2222-2222-2222-222222222222" eId="hauptteil-1_abschnitt-2_art-1_abs-5_inhalt-1">
                            <akn:p GUID="ea333333-3333-3333-3333-333333333333" eId="hauptteil-1_abschnitt-2_art-1_abs-5_inhalt-1_text-1">
                                Als Lebensmittelzusatzstoffe für die Verarbeitung von Erzeugnissen nach Anlage 1 sind ausschließlich die in Anlage 4 Abschnitt B aufgeführten Stoffe nach den dort definierten Maßgaben zugelassen.
                                <akn:marker GUID="ea444444-4444-4444-4444-444444444444" eId="hauptteil-1_abschnitt-2_art-1_abs-5_inhalt-1_text-1_marker-1" refersTo="satzende"/>
                            </akn:p>
                        </akn:content>
                    </akn:paragraph>
                    <akn:paragraph GUID="ea555555-5555-5555-5555-555555555555" eId="hauptteil-1_abschnitt-2_art-1_abs-6">
                        <akn:num GUID="ea666666-6666-6666-6666-666666666666" eId="hauptteil-1_abschnitt-2_art-1_abs-6_bezeichnung-1">(6)</akn:num>
                        <akn:content GUID="ea777777-7777-7777-7777-777777777777" eId="hauptteil-1_abschnitt-2_art-1_abs-6_inhalt-1">
                            <akn:p GUID="ea888888-8888-8888-8888-888888888888" eId="hauptteil-1_abschnitt-2_art-1_abs-6_inhalt-1_text-1">
                                Fruchtnektare müssen die in Anlage 5 vorgeschriebenen Mindestgehalte an Fruchtsaft oder Fruchtmark aufweisen.
                                <akn:marker GUID="ea999999-9999-9999-9999-999999999999" eId="hauptteil-1_abschnitt-2_art-1_abs-6_inhalt-1_text-1_marker-1" refersTo="satzende"/>
                            </akn:p>
                        </akn:content>
                    </akn:paragraph>
                    <akn:paragraph GUID="eb111111-1111-1111-1111-111111111111" eId="hauptteil-1_abschnitt-2_art-1_abs-7">
                        <akn:num GUID="eb222222-2222-2222-2222-222222222222" eId="hauptteil-1_abschnitt-2_art-1_abs-7_bezeichnung-1">(7)</akn:num>
                        <akn:content GUID="eb333333-3333-3333-3333-333333333333" eId="hauptteil-1_abschnitt-2_art-1_abs-7_inhalt-1">
                            <akn:p GUID="eb444444-4444-4444-4444-444444444444" eId="hauptteil-1_abschnitt-2_art-1_abs-7_inhalt-1_text-1">
                                Bei Fruchtsäften aus Fruchtsaftkonzentrat muss der Mindestbrixwert des wiederhergestellten Saftes dem Ursprungswert des verwendeten Konzentrats entsprechen.
                                <akn:marker GUID="eb555555-5555-5555-5555-555555555555" eId="hauptteil-1_abschnitt-2_art-1_abs-7_inhalt-1_text-1_marker-1" refersTo="satzende"/>
                            </akn:p>
                        </akn:content>
                    </akn:paragraph>
                    <akn:paragraph GUID="eb666666-6666-6666-6666-666666666666" eId="hauptteil-1_abschnitt-2_art-1_abs-8">
                        <akn:num GUID="eb777777-7777-7777-7777-777777777777" eId="hauptteil-1_abschnitt-2_art-1_abs-8_bezeichnung-1">(8)</akn:num>
                        <akn:content GUID="eb888888-8888-8888-8888-888888888888" eId="hauptteil-1_abschnitt-2_art-1_abs-8_inhalt-1">
                            <akn:p GUID="eb999999-9999-9999-9999-999999999999" eId="hauptteil-1_abschnitt-2_art-1_abs-8_inhalt-1_text-1">
                                Unberührt bleiben die Vorschriften über den Zusatz von Vitaminen und Mineralstoffen zu Lebensmitteln.
                                <akn:marker GUID="ec111111-1111-1111-1111-111111111111" eId="hauptteil-1_abschnitt-2_art-1_abs-8_inhalt-1_text-1_marker-1" refersTo="satzende"/>
                            </akn:p>
                        </akn:content>
                    </akn:paragraph>
                </akn:article>
                <akn:article GUID="ec222222-2222-2222-2222-222222222222" eId="hauptteil-1_abschnitt-2_art-2" period="#meta-1_geltzeiten-1_geltungszeitgr-24" refersTo="stammform">
                    <akn:num GUID="ec333333-3333-3333-3333-333333333333" eId="hauptteil-1_abschnitt-2_art-2_bezeichnung-1">
                        § 3
                    </akn:num>
                    <akn:heading GUID="ec444444-4444-4444-4444-444444444444" eId="hauptteil-1_abschnitt-2_art-2_überschrift-1">
                        Kennzeichnung
                    </akn:heading>
                    <akn:paragraph GUID="dccccccc-cccc-cccc-cccc-cccccccccccc" eId="hauptteil-1_abschnitt-2_art-2_abs-1">
                        <akn:num GUID="ddcccccc-cccc-cccc-cccc-cccccccccccc" eId="hauptteil-1_abschnitt-2_art-2_abs-1_bezeichnung-1">(1)</akn:num>
                        <akn:content GUID="decccccc-cccc-cccc-cccc-cccccccccccc" eId="hauptteil-1_abschnitt-2_art-2_abs-1_inhalt-1">
                            <akn:p GUID="dfcccccc-cccc-cccc-cccc-cccccccccccc" eId="hauptteil-1_abschnitt-2_art-2_abs-1_inhalt-1_text-1">
                                Die Kennzeichnung der Erzeugnisse erfolgt nach den aktuellen gesetzlichen Vorgaben. Alle wesentlichen Informationen wie Inhaltsstoffe, Mindesthaltbarkeitsdatum und Herkunft sind klar und deutlich auszuweisen.
                                <akn:marker GUID="d0cccccc-cccc-cccc-cccc-cccccccccccc" eId="hauptteil-1_abschnitt-2_art-2_abs-1_inhalt-1_text-1_marker-1" refersTo="satzende"/>
                            </akn:p>
                        </akn:content>
                    </akn:paragraph>
                </akn:article>
            </akn:section>
        </akn:body>
        <akn:conclusions GUID="71b6351e-5975-448a-8e10-46a27e34a73a" eId="schluss-1">
