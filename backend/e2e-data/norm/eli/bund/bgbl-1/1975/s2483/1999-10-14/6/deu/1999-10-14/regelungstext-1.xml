                    </akn:shortTitle>
                </akn:p>
            </akn:longTitle>
        </akn:preface>
        <akn:preamble GUID="55555555-6666-7777-8888-999999999900" eId="präambel-n1">
            <akn:formula GUID="66666666-7777-8888-9999-aaaaaaaaaabb" eId="präambel-n1_formel-n1"
                         name="attributsemantik-noch-undefiniert" refersTo="eingangsformel">
                <akn:p GUID="77777777-8888-9999-aaaa-bbbbbbbbbccc" eId="präambel-n1_formel-n1_text-n1">
                    Im Rahmen der gesetzlichen Bestimmungen wird diese Testverordnung zur Erprobung des Einsatzes von Fruchtnektar- und Fruchtsirupproduktionsverfahren bekannt gemacht.
                </akn:p>
            </akn:formula>
        </akn:preamble>
        <akn:body GUID="88888888-9999-aaaa-bbbb-ccccccccdddd" eId="hauptteil-n1">
            <!-- Artikel 1 -->
            <akn:article GUID="99999999-aaaa-bbbb-cccc-ddddddddddde" eId="art-z1" period="#meta-n1_geltzeiten-n1_geltungszeitgr-n10" refersTo="stammform">
                <akn:num GUID="aaaaaaaa-bbbb-cccc-dddd-eeeeeeeeeeee" eId="art-z1_bezeichnung-n1">§ 1</akn:num>
                <akn:heading GUID="bbbbbbbb-cccc-dddd-eeee-ffffffffffff" eId="art-z1_überschrift-n1">
                    Begriffsbestimmungen
                </akn:heading>
                <akn:paragraph GUID="cccccccc-dddd-eeee-ffff-111111111111" eId="art-z1_abs-z1">
                    <akn:num GUID="dddddddd-eeee-ffff-0000-222222222222" eId="art-z1_abs-z1_bezeichnung-n1">(1)</akn:num>
                    <akn:content GUID="eeeeeeee-ffff-0000-1111-333333333333" eId="art-z1_abs-z1_inhalt-n1">
                        <akn:p GUID="ffffffff-0000-1111-2222-444444444444" eId="art-z1_abs-z1_inhalt-n1_text-n1">
                            In dieser Testverordnung bezeichnet „Fruchtnektar“ ein nicht gegorenes, gärfähiges Getränk, das aus Fruchtsaft oder Fruchtmark hergestellt und zur Prüfung innovativer Produktionsverfahren dient.
                        </akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
            <!-- Artikel 2 -->
            <akn:article GUID="11111111-2222-3333-4444-555555555555" eId="art-z2" period="#meta-n1_geltzeiten-n1_geltungszeitgr-n12" refersTo="stammform">
                <akn:num GUID="22222222-3333-4444-5555-666666666666" eId="art-z2_bezeichnung-n1">§ 2</akn:num>
                <akn:heading GUID="33333333-4444-5555-6666-777777777777" eId="art-z2_überschrift-n1">
                    Herstellung von Fruchtnektar
                </akn:heading>
                <akn:paragraph GUID="44444444-5555-6666-7777-888888888888" eId="art-z2_abs-z1">
                    <akn:num GUID="55555555-6666-7777-8888-999999999999" eId="art-z2_abs-z1_bezeichnung-n1">(1)</akn:num>
                    <akn:content GUID="66666666-7777-8888-9999-aaaaaaaaaaaa" eId="art-z2_abs-z1_inhalt-n1">
                        <akn:p GUID="77777777-8888-9999-aaaa-bbbbbbbbbbbb" eId="art-z2_abs-z1_inhalt-n1_text-n1">
                            Der Fruchtnektar wird unter Verwendung standardisierter Verfahren in geprüften Anlagen hergestellt.
                        </akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
            <!-- Artikel 3 -->
            <akn:article GUID="88888888-9999-aaaa-bbbb-cccccccccccc" eId="art-z3" period="#meta-n1_geltzeiten-n1_geltungszeitgr-n12" refersTo="stammform">
                <akn:num GUID="99999999-aaaa-bbbb-cccc-dddddddddddd" eId="art-z3_bezeichnung-n1">§ 3</akn:num>
                <akn:heading GUID="aaaaaaaa-bbbb-cccc-dddd-eeeeeeeeeeee" eId="art-z3_überschrift-n1">
                    Herstellung von Fruchtsirup
                </akn:heading>
                <akn:paragraph GUID="bbbbbbbb-cccc-dddd-eeee-ffffffffffff" eId="art-z3_abs-z1">
                    <akn:num GUID="cccccccc-dddd-eeee-ffff-111111111111" eId="art-z3_abs-z1_bezeichnung-n1">(1)</akn:num>
                    <akn:content GUID="dddddddd-eeee-ffff-0000-222222222222" eId="art-z3_abs-z1_inhalt-n1">
                        <akn:p GUID="eeeeeeee-ffff-0000-1111-333333333333" eId="art-z3_abs-z1_inhalt-n1_text-n1">
                            Fruchtsirup wird durch Verdampfung von Fruchtsaft und anschließende Zugabe von Verdickungsmitteln unter kontrollierten Bedingungen hergestellt.
                        </akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
            <!-- Artikel 4 -->
            <akn:article GUID="ffffffff-0000-1111-2222-333333333333" eId="art-z4" period="#meta-n1_geltzeiten-n1_geltungszeitgr-n2" refersTo="stammform">
                <akn:num GUID="11111111-2222-3333-4444-444444444444" eId="art-z4_bezeichnung-n1">§ 4</akn:num>
                <akn:heading GUID="22222222-3333-4444-5555-555555555555" eId="art-z4_überschrift-n1">
                    Geräteüberwachung
                </akn:heading>
                <akn:paragraph GUID="33333333-4444-5555-6666-666666666666" eId="art-z4_abs-z1">
                    <akn:num GUID="44444444-5555-6666-7777-777777777777" eId="art-z4_abs-z1_bezeichnung-n1">(1)</akn:num>
                    <akn:content GUID="55555555-6666-7777-8888-888888888888" eId="art-z4_abs-z1_inhalt-n1">
                        <akn:p GUID="66666666-7777-8888-9999-999999999999" eId="art-z4_abs-z1_inhalt-n1_text-n1">
                            Die ordnungsgemäße Funktion der Maschinen und Instrumente, die im Rahmen der Herstellung eingesetzt werden, wird regelmäßig geprüft.
                        </akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
            <!-- Artikel 5 -->
            <akn:article GUID="77777777-8888-9999-aaaa-aaaaaaaaaaaa" eId="art-z5" period="#meta-n1_geltzeiten-n1_geltungszeitgr-n2" refersTo="stammform">
                <akn:num GUID="88888888-9999-aaaa-bbbb-bbbbbbbbbbbb" eId="art-z5_bezeichnung-n1">§ 5</akn:num>
                <akn:heading GUID="99999999-aaaa-bbbb-cccc-cccccccccccc" eId="art-z5_überschrift-n1">
                    Qualitätskontrolle
                </akn:heading>
                <akn:paragraph GUID="aaaaaaaa-bbbb-cccc-dddd-dddddddddddd" eId="art-z5_abs-z1">
                    <akn:num GUID="bbbbbbbb-cccc-dddd-eeee-eeeeeeeeeeee" eId="art-z5_abs-z1_bezeichnung-n1">(1)</akn:num>
                    <akn:content GUID="cccccccc-dddd-eeee-ffff-ffffffffffff" eId="art-z5_abs-z1_inhalt-n1">
                        <akn:p GUID="dddddddd-eeee-ffff-0000-111111111111" eId="art-z5_abs-z1_inhalt-n1_text-n1">
                            Alle Produktionspartien werden stichprobenartig auf die Einhaltung der Qualitätsstandards hin geprüft.
                        </akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
            <!-- Artikel 6 -->
            <akn:article GUID="eeeeeeee-ffff-0000-1111-222222222222" eId="art-z6" period="#meta-n1_geltzeiten-n1_geltungszeitgr-n2" refersTo="stammform">
                <akn:num GUID="ffffffff-0000-1111-2222-333333333333" eId="art-z6_bezeichnung-n1">§ 6</akn:num>
                <akn:heading GUID="11111111-2222-3333-4444-444444444445" eId="art-z6_überschrift-n1">
                    Sonderregelungen
                </akn:heading>
                <akn:paragraph GUID="22222222-3333-4444-5555-555555555556" eId="art-z6_abs-z1">
                    <akn:num GUID="33333333-4444-5555-6666-666666666667" eId="art-z6_abs-z1_bezeichnung-n1">(1)</akn:num>
                    <akn:content GUID="44444444-5555-6666-7777-777777777778" eId="art-z6_abs-z1_inhalt-n1">
                        <akn:p GUID="55555555-6666-7777-8888-888888888889" eId="art-z6_abs-z1_inhalt-n1_text-n1">
                            Für besondere Produktionsverfahren gelten abweichende Regelungen, die im Einzelfall von der Aufsichtsbehörde genehmigt werden.
                        </akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
            <!-- Artikel 7 -->
            <akn:article GUID="66666666-7777-8888-9999-999999999999" eId="art-z7" period="#meta-n1_geltzeiten-n1_geltungszeitgr-n2" refersTo="stammform">
                <akn:num GUID="77777777-8888-9999-aaaa-aaaaaaaaaaab" eId="art-z7_bezeichnung-n1">§ 7</akn:num>
                <akn:heading GUID="88888888-9999-aaaa-bbbb-bbbbbbbbbbbc" eId="art-z7_überschrift-n1">
                    Prüf- und Kontrollverfahren
                </akn:heading>
                <akn:paragraph GUID="99999999-aaaa-bbbb-cccc-cccccccccccc" eId="art-z7_abs-z1">
                    <akn:num GUID="aaaaaaaa-bbbb-cccc-dddd-dddddddddddd" eId="art-z7_abs-z1_bezeichnung-n1">(1)</akn:num>
                    <akn:content GUID="bbbbbbbb-cccc-dddd-eeee-eeeeeeeeeeee" eId="art-z7_abs-z1_inhalt-n1">
                        <akn:p GUID="cccccccc-dddd-eeee-ffff-ffffffffffff" eId="art-z7_abs-z1_inhalt-n1_text-n1">
                            Die Einhaltung der Produktions- und Sicherheitsvorschriften wird durch interne und externe Kontrollen sichergestellt.
                        </akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
            <!-- Artikel 8 -->
            <akn:article GUID="dddddddd-eeee-ffff-0000-111111111112" eId="art-z8" period="#meta-n1_geltzeiten-n1_geltungszeitgr-n8" refersTo="stammform">
                <akn:num GUID="eeeeeeee-ffff-0000-1111-222222222223" eId="art-z8_bezeichnung-n1">§ 8</akn:num>
                <akn:heading GUID="ffffffff-0000-1111-2222-333333333334" eId="art-z8_überschrift-n1">
                    Besondere Auflagen
                </akn:heading>
                <akn:paragraph GUID="11111111-2222-3333-4444-444444444444" eId="art-z8_abs-z1">
                    <akn:num GUID="22222222-3333-4444-5555-555555555555" eId="art-z8_abs-z1_bezeichnung-n1">(1)</akn:num>
                    <akn:content GUID="33333333-4444-5555-6666-666666666666" eId="art-z8_abs-z1_inhalt-n1">
                        <akn:p GUID="44444444-5555-6666-7777-777777777777" eId="art-z8_abs-z1_inhalt-n1_text-n1">
                            Die zuständige Behörde kann in Einzelfällen von den allgemeinen Vorschriften abweichen und spezifische Auflagen erlassen.
                        </akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
            <!-- Artikel 9 -->
            <akn:article GUID="55555555-6666-7777-8888-888888888888" eId="art-z9" period="#meta-n1_geltzeiten-n1_geltungszeitgr-n8" refersTo="stammform">
                <akn:num GUID="66666666-7777-8888-9999-999999999999" eId="art-z9_bezeichnung-n1">§ 9</akn:num>
                <akn:heading GUID="77777777-8888-9999-aaaa-bbbbbbbbbbbb" eId="art-z9_überschrift-n1">
                    Übergangsregelungen
                </akn:heading>
                <akn:paragraph GUID="88888888-9999-aaaa-bbbb-cccccccccccc" eId="art-z9_abs-z1">
                    <akn:num GUID="99999999-aaaa-bbbb-cccc-dddddddddddd" eId="art-z9_abs-z1_bezeichnung-n1">(1)</akn:num>
                    <akn:content GUID="aaaaaaaa-bbbb-cccc-dddd-eeeeeeeeeeee" eId="art-z9_abs-z1_inhalt-n1">
                        <akn:p GUID="bbbbbbbb-cccc-dddd-eeee-ffffffffffff" eId="art-z9_abs-z1_inhalt-n1_text-n1">
                            In Übergangsfällen gelten die zum Zeitpunkt des Inkrafttretens maßgeblichen Regelungen.
                        </akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
            <!-- Artikel 10 -->
            <akn:article GUID="cccccccc-dddd-eeee-ffff-111111111111" eId="art-z10" period="#meta-n1_geltzeiten-n1_geltungszeitgr-n10" refersTo="stammform">
                <akn:num GUID="dddddddd-eeee-ffff-0000-222222222222" eId="art-z10_bezeichnung-n1">§ 10</akn:num>
                <akn:heading GUID="eeeeeeee-ffff-0000-1111-333333333333" eId="art-z10_überschrift-n1">
                    Ordnungswidrigkeiten
                </akn:heading>
                <akn:paragraph GUID="ffffffff-0000-1111-2222-333333333333" eId="art-z10_abs-n1">
                    <akn:num GUID="11111111-2222-3333-4444-444444444444" eId="art-z10_abs-n1_bezeichnung-n1">(1)</akn:num>
                    <akn:content GUID="22222222-3333-4444-5555-555555555555" eId="art-z10_abs-n1_inhalt-n1">
                        <akn:p GUID="33333333-4444-5555-6666-666666666666" eId="art-z10_abs-n1_inhalt-n1_text-n1">
                            Ordnungswidrig handelt, wer in Verstoß gegen diese Testverordnung handelt.
                        </akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
            <!-- Artikel 11 -->
            <akn:article GUID="44444444-5555-6666-7777-777777777777" eId="art-z11" period="#meta-n1_geltzeiten-n1_geltungszeitgr-n8" refersTo="stammform">
                <akn:num GUID="55555555-6666-7777-8888-888888888888" eId="art-z11_bezeichnung-n1">§ 11</akn:num>
                <akn:heading GUID="66666666-7777-8888-9999-999999999999" eId="art-z11_überschrift-n1"/>
                <akn:paragraph GUID="77777777-8888-9999-aaaa-bbbbbbbbbbbb" eId="art-z11_abs-z1">
                    <akn:num GUID="88888888-9999-aaaa-bbbb-cccccccccccc" eId="art-z11_abs-z1_bezeichnung-n1"/>
                    <akn:content GUID="99999999-aaaa-bbbb-cccc-dddddddddddd" eId="art-z11_abs-z1_inhalt-n1">
                        <akn:p GUID="aaaaaaaa-bbbb-cccc-dddd-eeeeeeeeeeee" eId="art-z11_abs-z1_inhalt-n1_text-n1">
                            Diese Testverordnung tritt am Tag nach der Veröffentlichung in Kraft.
                        </akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
        </akn:body>
        <akn:conclusions GUID="bbbbbbbb-cccc-dddd-eeee-ffffffffffff" eId="schluss-n1">
            <akn:formula GUID="cccccccc-dddd-eeee-ffff-111111111122" eId="schluss-n1_formel-n1"
                         name="attributsemantik-noch-undefiniert" refersTo="schlussformel">
                <akn:p GUID="dddddddd-eeee-ffff-0000-222222222233" eId="schluss-n1_formel-n1_text-n1">
                    Diese Testverordnung wird unmittelbar nach ihrer Verkündung wirksam.
                </akn:p>
            </akn:formula>
        </akn:conclusions>
    </akn:act>
</akn:akomaNtoso>
