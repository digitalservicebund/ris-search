            <!-- Artikel 1 -->
            <akn:article GUID="99999999-aaaa-bbbb-cccc-ddddddddddde" eId="hauptteil-1_art-1" period="#meta-1_geltzeiten-1_geltungszeitgr-10" refersTo="stammform">
                <akn:num GUID="aaaaaaaa-bbbb-cccc-dddd-eeeeeeeeeeee" eId="hauptteil-1_art-1_bezeichnung-1">Â§ 1</akn:num>
                <akn:heading GUID="bbbbbbbb-cccc-dddd-eeee-ffffffffffff" eId="hauptteil-1_art-1_Ã¼berschrift-1">
                    Begriffsbestimmungen
                </akn:heading>
                <akn:paragraph GUID="cccccccc-dddd-eeee-ffff-111111111111" eId="hauptteil-1_art-1_abs-1">
                    <akn:num GUID="dddddddd-eeee-ffff-0000-222222222222" eId="hauptteil-1_art-1_abs-1_bezeichnung-1">(1)</akn:num>
                    <akn:content GUID="eeeeeeee-ffff-0000-1111-333333333333" eId="hauptteil-1_art-1_abs-1_inhalt-1">
                        <akn:p GUID="ffffffff-0000-1111-2222-444444444444" eId="hauptteil-1_art-1_abs-1_inhalt-1_text-1">
                            In dieser Testverordnung bezeichnet âFruchtnektarâ ein nicht gegorenes, gÃ¤rfÃ¤higes GetrÃ¤nk, das aus Fruchtsaft oder Fruchtmark hergestellt und zur PrÃ¼fung innovativer Produktionsverfahren dient.
                        </akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
            <!-- Artikel 2 -->
            <akn:article GUID="11111111-2222-3333-4444-555555555555" eId="hauptteil-1_art-2" period="#meta-1_geltzeiten-1_geltungszeitgr-12" refersTo="stammform">
                <akn:num GUID="22222222-3333-4444-5555-666666666666" eId="hauptteil-1_art-2_bezeichnung-1">Â§ 2</akn:num>
                <akn:heading GUID="33333333-4444-5555-6666-777777777777" eId="hauptteil-1_art-2_Ã¼berschrift-1">
                    Herstellung von Fruchtnektar
                </akn:heading>
                <akn:paragraph GUID="44444444-5555-6666-7777-888888888888" eId="hauptteil-1_art-2_abs-1">
                    <akn:num GUID="55555555-6666-7777-8888-999999999999" eId="hauptteil-1_art-2_abs-1_bezeichnung-1">(1)</akn:num>
                    <akn:content GUID="66666666-7777-8888-9999-aaaaaaaaaaaa" eId="hauptteil-1_art-2_abs-1_inhalt-1">
                        <akn:p GUID="77777777-8888-9999-aaaa-bbbbbbbbbbbb" eId="hauptteil-1_art-2_abs-1_inhalt-1_text-1">
                            Der Fruchtnektar wird unter Verwendung standardisierter Verfahren in geprÃ¼ften Anlagen hergestellt.
                        </akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
            <!-- Artikel 3 -->
            <akn:article GUID="88888888-9999-aaaa-bbbb-cccccccccccc" eId="hauptteil-1_art-3" period="#meta-1_geltzeiten-1_geltungszeitgr-12" refersTo="stammform">
