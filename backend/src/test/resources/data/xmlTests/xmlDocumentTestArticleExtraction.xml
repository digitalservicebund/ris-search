        <akn:body GUID="2521d678-5a5e-4746-837b-594cb1b2c1b9" eId="hauptteil-1">
            <akn:article GUID="c4e78387-33af-46ce-b6be-75bbc164b055" eId="hauptteil-1_abschnitt-1_art-1" period="#meta-1_geltzeiten-1_geltungszeitgr-1" refersTo="stammform">
                <akn:num GUID="3a03d37f-3607-439f-8c16-4cb54abc3800" eId="hauptteil-1_abschnitt-1_art-1_bezeichnung-1">§ 1</akn:num>
                <akn:heading GUID="db338584-a78e-4333-87d7-b1a59d33ee94"
                             eId="hauptteil-1_abschnitt-1_art-1_überschrift-1">Heading
                    <akn:authorialNote>Heading footnote.</akn:authorialNote>
                </akn:heading>
                <akn:paragraph GUID="023731ab-d7b4-4b83-bd5c-629f80adcc31" eId="hauptteil-1_abschnitt-1_art-1_abs-1">
                    <akn:num GUID="36e32158-cea4-44b5-9db2-2aa1e1df50bc" eId="hauptteil-1_abschnitt-1_art-1_abs-1_bezeichnung-1"/>
                    <akn:content GUID="e5c28499-9bec-40bf-a419-9aeb7a442d0f" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1">
                        <akn:p GUID="fc273a32-64e4-48ce-bf16-679103cb3bc5" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_text-1">
                            Das ist ein Satz.
                            <akn:marker GUID="567e770d-79c2-4f70-bd6b-09e96d0cdca1" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_text-1_marker-1" refersTo="satzende"/>
                            Das ist noch ein Satz.
                            <akn:marker GUID="4b061102-d668-47fe-bce4-f1aa01719910" eId="hauptteil-1_abschnitt-1_art-1_abs-1_inhalt-1_text-1_marker-2" refersTo="satzende"/>
                        </akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
            <akn:article GUID="f3d05406-8679-4e3e-9678-ba0cf42d71ba" eId="hauptteil-1_abschnitt-1_art-2" period="#meta-1_geltzeiten-1_geltungszeitgr-2" refersTo="stammform">
                <akn:num GUID="1f72458f-e8a7-47d7-9bf9-61a952ba9abc" eId="hauptteil-1_abschnitt-1_art-2_bezeichnung-1">§ 2</akn:num>
                <akn:heading GUID="7970777d-8ac5-4737-9545-126e46765180" eId="hauptteil-1_abschnitt-1_art-2_überschrift-1">Heading<akn:br /> 2</akn:heading>
                <akn:paragraph GUID="40f53f0f-f083-46ba-8654-f91f977b1a39" eId="hauptteil-1_abschnitt-1_art-2_abs-1">
                    <akn:num GUID="91777ce6-5731-4ef1-93c0-7996349f6c3b" eId="hauptteil-1_abschnitt-1_art-2_abs-1_bezeichnung-1">(1)</akn:num>
                    <akn:content GUID="2fac68dc-fbdc-4414-afab-baef6f54288a" eId="hauptteil-1_abschnitt-1_art-2_abs-1_inhalt-1">
                        <akn:p GUID="1328fb28-4b2d-48bf-a0ad-cfcf46699346" eId="hauptteil-1_abschnitt-1_art-2_abs-1_inhalt-1_text-1">
                            Ein weiterer Satz mit
                            <akn:marker GUID="d62ea358-2e70-4b4c-9cde-75586b5a4d19" eId="hauptteil-1_abschnitt-1_art-2_abs-1_inhalt-1_text-1_marker-1" refersTo="satzende"/>
                        </akn:p>
                        <akn:blockList GUID="96be476d-310e-42f3-9d0d-16c940a62c32"
                                       eId="hauptteil-1_abschnitt-1_art-2_abs-1_inhalt-1_text-1_liste-1">
                            <akn:item GUID="a977a83c-66b3-4cde-86a2-d001b3dc8c75"
                                      eId="hauptteil-1_abschnitt-1_art-2_abs-1_inhalt-1_text-1_liste-1_listenelem-1">
                                <akn:num GUID="63e6fe92-de7f-4570-b068-f5fba0f18232"
                                         eId="hauptteil-1_abschnitt-1_art-2_abs-1_inhalt-1_text-1_liste-1_listenelem-1_bezeichnung-1">
                                    a)
                                </akn:num>
                                <akn:p GUID="e0589b09-674a-4b7a-9775-01ac9d39967a"
                                       eId="hauptteil-1_abschnitt-1_art-2_abs-1_inhalt-1_text-1_liste-1_listenelem-1_text-1">
                                    einem Punkt in einer Aufzählung und
                                </akn:p>
                            </akn:item>
                            <akn:item GUID="dc4b4f29-7103-4761-92f0-b3a18862da5b"
                                      eId="hauptteil-1_abschnitt-1_art-2_abs-1_inhalt-1_text-1_liste-1_listenelem-2">
                                <akn:num GUID="b5bba982-8b48-419e-9192-bae03abbec0c"
                                         eId="hauptteil-1_abschnitt-1_art-2_abs-1_inhalt-1_text-1_liste-1_listenelem-2_bezeichnung-1">
                                    b)
                                </akn:num>
                                <akn:p GUID="d6e76b20-1a0f-4190-b269-e2e3c372f646"
                                       eId="hauptteil-1_abschnitt-1_art-2_abs-1_inhalt-1_text-1_liste-1_listenelem-2_text-1">
                                    noch einem Punkt.
                                    <akn:marker GUID="9d2de3d7-9cf8-404d-974c-afc36f5b364c"
                                                eId="hauptteil-1_abschnitt-1_art-2_abs-1_inhalt-1_text-1_liste-1_listenelem-2_text-1_marker-1"
                                                refersTo="satzende"/>
                                </akn:p>
                            </akn:item>
                        </akn:blockList>
                    </akn:content>
                </akn:paragraph>
                <akn:paragraph GUID="c8e06b30-109f-487e-9536-bb7bb5651a68" eId="hauptteil-1_abschnitt-1_art-2_abs-2">
                    <akn:num GUID="bd0671ba-596e-4ef4-9307-2e098525cdaf" eId="hauptteil-1_abschnitt-1_art-2_abs-2_bezeichnung-1">(2)</akn:num>
                    <akn:content GUID="d95a4a67-d299-4d61-bd0c-69b53b97abca" eId="hauptteil-1_abschnitt-1_art-2_abs-2_inhalt-1">
                        <akn:p GUID="994b5c7e-ad60-4a2a-a40a-ed10f54df1c3" eId="hauptteil-1_abschnitt-1_art-2_abs-2_inhalt-1_text-1">
                            Noch ein wichtiger Satz.
                            <akn:marker GUID="e1c2104a-083a-4b51-8ba1-bbdb7e7a92a8" eId="hauptteil-1_abschnitt-1_art-2_abs-2_inhalt-1_text-1_marker-1" refersTo="satzende"/>
                            Das ist der letzte Satz.
                            <akn:marker GUID="8e6a4893-b200-421a-b962-f3219d4b253f" eId="hauptteil-1_abschnitt-1_art-2_abs-2_inhalt-1_text-1_marker-2" refersTo="satzende"/>
                        </akn:p>
                    </akn:content>
                </akn:paragraph>
            </akn:article>
