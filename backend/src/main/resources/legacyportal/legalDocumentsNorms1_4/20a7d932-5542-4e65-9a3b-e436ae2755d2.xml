                <akn:heading GUID="60e7e533-1d4c-4b40-bc98-78b70cf91709" eId="para-10_überschrift-1">

                </akn:heading>
                <akn:paragraph GUID="ba92de1c-fe19-44eb-85dc-7d03146f45f0" eId="para-10_abs-0">
                    <akn:num GUID="3c1a9da3-31d1-4f6d-aece-321382c19b6a" eId="para-10_abs-0_bezeichnung-1">
                        <akn:marker name="0" />

                    </akn:num>
                    <akn:content GUID="b231f94d-d4df-4adf-8ab5-9687d5f719fa" eId="para-10_abs-0_inhalt-1">
                        <akn:p GUID="7108e58b-823f-4210-8e38-dae93981c14d" eId="para-10_abs-0_inhalt-1_text-1">
                            -
                        </akn:p>
                    </akn:content>


                </akn:paragraph>
            </akn:article>
            <akn:article refersTo="einzelvorschrift-stammform" GUID="b342edba-7cf4-4e0d-ba83-038a0a5829be" eId="para-11">
                <akn:num GUID="71ed4b10-2da2-4d73-b867-9e3b83f9f80f" eId="para-11_bezeichnung-1">
                    <akn:marker name="11" /> § 11 </akn:num>
                <akn:heading GUID="eea42fab-6304-4572-a6b8-b3213760c67c" eId="para-11_überschrift-1">

                </akn:heading>
                <akn:paragraph GUID="e4b29dc1-10e9-4f12-95a3-53932e197af5" eId="para-11_abs-0">
                    <akn:num GUID="61b4aa04-66be-4b21-a55b-d86123e71436" eId="para-11_abs-0_bezeichnung-1">
                        <akn:marker name="0" />

                    </akn:num>
                    <akn:content GUID="6780b5b3-84d2-4034-b575-d66d42e40844" eId="para-11_abs-0_inhalt-1">
                        <akn:p GUID="e765c50d-5363-4c15-b2f9-97370c19c5ff" eId="para-11_abs-0_inhalt-1_text-1">
                            Dieses Gesetz gilt nach Maßgabe des § 13 Abs. 1 des Dritten Überleitungsgesetzes vom 4. Januar 1952 (Bundesgesetzbl. I S.
                            1) auch im Land Berlin.
                        </akn:p>
                    </akn:content>


                </akn:paragraph>
            </akn:article>
            <akn:article refersTo="einzelvorschrift-stammform" GUID="58fcf23d-2af5-4f71-bed8-333402a4ce28" eId="para-12">
