                        <akn:p GUID="9d6fb654-cd05-4d64-bd00-b03acf0289e2" eId="para-9_abs-0_inhalt-1_text-1">
                            -
                        </akn:p>
                    </akn:content>


                </akn:paragraph>
            </akn:article>
            <akn:article refersTo="einzelvorschrift-stammform" GUID="43f71556-1e00-4c96-8078-d1b24b830461" eId="para-10">
                <akn:num GUID="b8acf5c7-8ca0-469d-8dce-3ba3ef041de5" eId="para-10_bezeichnung-1">
