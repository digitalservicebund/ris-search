{
  "name": "ris-portal-frontend",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "build": "nuxt build",
    "test": "TZ=UTC vitest run",
    "test:watch": "TZ=UTC vitest",
    "coverage": "TZ=UTC vitest run --coverage",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare": "nuxt prepare",
    "fmt": "prettier . --write --ignore-unknown",
    "lint": "oxlint --fix",
    "style:check": "prettier --check && oxlint --max-warnings=0",
    "style:fix": "pnpm fmt && pnpm lint",
    "typecheck": "nuxt typecheck",
    "audit:licences": "bash -c 'license-checker-rseidelsohn --production --excludePrivatePackages --onlyAllow \"$(./parseAllowedLicences.sh)\" --csv --out ./frontend-licence-report.csv'"
  },
  "engines": {
    "node": "<=24",
    "pnpm": "<=10"
  },
  "packageManager": "pnpm@10.28.1",
  "dependencies": {
    "@digitalservicebund/ris-ui": "3.21.3",
    "@nuxtjs/mdc": "0.20.0",
    "@nuxtjs/sitemap": "7.6.0",
    "@sentry/nuxt": "10.38.0",
    "dayjs": "1.11.19",
    "lodash": "4.17.23",
    "minisearch": "7.2.0",
    "nuxt": "4.2.2",
    "nuxt-security": "2.5.1",
    "posthog-js": "1.334.1",
    "primevue": "4.5.4",
    "sanitize-html": "2.17.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
    "@iconify-json/ic": "1.2.4",
    "@iconify/json": "2.2.432",
    "@nuxt/test-utils": "4.0.0",
    "@playwright/test": "1.58.2",
    "@tailwindcss/vite": "4.1.18",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/user-event": "14.6.1",
    "@testing-library/vue": "8.1.0",
    "@types/jsdom": "27.0.0",
    "@types/lodash": "4.17.23",
    "@types/node": "25.0.10",
    "@types/sanitize-html": "2.16.0",
    "@vitest/coverage-v8": "4.0.17",
    "@vue/test-utils": "2.4.6",
    "axe-html-reporter": "2.2.11",
    "jsdom": "28.0.0",
    "license-checker-rseidelsohn": "4.4.2",
    "lighthouse": "13.0.3",
    "oxlint": "1.43.0",
    "playwright-lighthouse": "4.0.0",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "unplugin-icons": "23.0.1",
    "vitest": "4.0.17",
    "vue-tsc": "3.2.4"
  }
