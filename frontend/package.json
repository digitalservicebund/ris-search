{
  "name": "ris-portal-frontend",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "build": "nuxt build",
    "coverage": "TZ=UTC vitest run --coverage",
    "test": "TZ=UTC vitest run",
    "test:watch": "TZ=UTC vitest",
    "e2e:seo": "playwright test e2e/seo.spec.ts --project chromium --workers=1",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare": "nuxt prepare",
    "eslint:check": "eslint --max-warnings 0",
    "eslint:fix": "eslint --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier . --write --ignore-unknown",
    "style:check": "pnpm prettier:check && pnpm eslint:check",
    "style:fix": "pnpm prettier:fix && pnpm eslint:fix",
    "oxlint:check": "oxlint --max-warnings=0",
    "oxlint:fix": "oxlint --fix",
    "typecheck": "nuxt typecheck",
    "audit:licences": "bash -c 'license-checker-rseidelsohn --production --excludePrivatePackages --onlyAllow \"$(./parseAllowedLicences.sh)\" --csv --out ./frontend-licence-report.csv'"
  },
  "engines": {
    "node": "24.13.0",
    "npm": "11.6.0"
  },
  "packageManager": "pnpm@10.28.1",
  "dependencies": {
    "@digitalservicebund/ris-ui": "3.21.1",
    "@nuxtjs/mdc": "0.20.0",
    "@nuxtjs/sitemap": "7.5.2",
    "@scalar/api-reference": "1.43.8",
    "@sentry/nuxt": "10.36.0",
    "dayjs": "1.11.19",
    "lodash": "4.17.21",
    "minisearch": "7.2.0",
    "nuxt": "4.2.2",
    "nuxt-security": "2.5.1",
    "posthog-js": "1.334.1",
    "primevue": "4.5.4",
    "sanitize-html": "2.17.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
    "@iconify-json/ic": "1.2.4",
    "@iconify/json": "2.2.432",
    "@nuxt/eslint": "1.12.1",
    "@nuxt/test-utils": "3.23.0",
    "@playwright/test": "1.57.0",
    "@tailwindcss/vite": "4.1.18",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/user-event": "14.6.1",
    "@testing-library/vue": "8.1.0",
    "@types/jsdom": "27.0.0",
    "@types/lodash": "4.17.23",
    "@types/node": "25.0.9",
    "@types/sanitize-html": "2.16.0",
    "@vitest/coverage-v8": "4.0.17",
    "@vue/test-utils": "2.4.6",
    "axe-html-reporter": "2.2.11",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-unused-imports": "4.3.0",
    "jsdom": "27.4.0",
    "license-checker-rseidelsohn": "4.4.2",
    "lighthouse": "13.0.1",
    "oxlint": "1.42.0",
    "playwright-lighthouse": "4.0.0",
    "prettier": "3.8.0",
    "prettier-plugin-tailwindcss": "0.7.2",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "unplugin-icons": "23.0.1",
    "vitest": "4.0.17",
    "vue-tsc": "3.2.2"
